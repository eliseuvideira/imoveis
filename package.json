    "husky": "^6.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1",
    "ts-node": "^10.4.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@ev-fns/dotenv": "^1.0.1",
    "@ev-fns/errors": "^0.1.1",
    "@ev-fns/server": "^0.2.1",
    "@ev-graphql-2/create-apollo": "^0.2.2",
    "@ev-graphql-2/create-resolver": "^0.1.2",
    "apollo-server-core": "^3.4.0",
    "apollo-server-express": "^3.4.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^15.5.3",
    "knex": "^0.95.13",
    "morgan": "^1.10.0",
    "pg": "^8.7.1"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev src/server.ts",
    "docker": "./scripts/docker.js",
    "format": "prettier --write 'src/**/*'",
    "lint": "eslint --max-warnings=0 'src/**/*'",
    "migrate-down": "knex --knexfile ./database/knexfile.js migrate:down",
    "migrate-latest": "knex --knexfile ./database/knexfile.js migrate:latest",
    "migrate-make": "knex --knexfile ./database/knexfile.js migrate:make",
    "migrate-rollback": "knex --knexfile ./database/knexfile.js migrate:rollback",
    "migrate-up": "knex --knexfile ./database/knexfile.js migrate:up",
    "start": "node build/server.js",
    "prepare": "husky install"
  }
}
